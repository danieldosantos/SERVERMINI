# Normaliza aliases dos livros de classe/subclasse para os identificadores can√¥nicos.
function classAliasCanonical(raw: text) :: text:
  set {_t} to {_raw}
  set {_t} to lowercase {_t}
  replace all " " with "" in {_t}
  replace all "-" with "" in {_t}
  if {_t} is "barbaro":
    set {_t} to "barbarian"
  if {_t} is "bardo":
    set {_t} to "bard"
  if {_t} is "bruxo":
    set {_t} to "warlock"
